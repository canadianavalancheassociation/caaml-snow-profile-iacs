	<simpleType name="ImpurityMethodOfMeasType">
		<union memberTypes="caaml:ImpurityMethodOfMeasEnumType gml:NilReasonEnumeration"/>
	</simpleType>
	<simpleType name="ImpurityMethodOfMeasEnumType">
		<restriction base="string">
			<enumeration value="NA"/>
		</restriction>
	</simpleType>
	<simpleType name="ImpurityType">
		<union memberTypes="caaml:ImpurityEnumType gml:NilReasonEnumeration"/>
	</simpleType>
	<simpleType name="ImpurityEnumType">
