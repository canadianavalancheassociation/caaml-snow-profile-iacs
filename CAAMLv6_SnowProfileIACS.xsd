	</complexType>
	<simpleType name="StrengthMethodOfMeasType">
		<union memberTypes="caaml:StrengthMethodOfMeasEnumType gml:NilReasonEnumeration"/>
	</simpleType>
	<simpleType name="StrengthMethodOfMeasEnumType">
		<restriction base="string">
