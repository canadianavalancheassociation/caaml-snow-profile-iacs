			<element name="hardnessMetaData">
				<complexType>
					<complexContent>
						<extension base="caaml:MetaDataBaseType">
							<sequence>
								<element name="methodOfMeas" type="caaml:HardnessMethodOfMeasType">
									<annotation>
										<documentation>Property element for numerical value of mass fraction of impurity (% or ppm)</documentation>
									</annotation>
								</element>
