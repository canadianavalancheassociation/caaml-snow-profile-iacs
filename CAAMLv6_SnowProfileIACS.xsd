			<enumeration value="compressive"/>
			<enumeration value="tensile"/>
			<enumeration value="shear"/>
		</restriction>
	</simpleType>
	<!-- EMBEDDED IMPURITY PROFILES -->
	<complexType name="ImpurityProfileType" final="#all">
