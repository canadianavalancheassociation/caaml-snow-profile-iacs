					<complexContent>
						<extension base="caaml:MetaDataBaseType">
							<sequence>
								<element name="impurity" type="caaml:ImpurityType">
									<annotation>
										<documentation>Type of impurity</documentation>
									</annotation>
								</element>
								<element name="methodOfMeas" type="caaml:ImpurityMethodOfMeasType">
									<annotation>
										<documentation>Property element for numerical value of mass fraction of impurity (% or ppm)</documentation>
									</annotation>
								</element>
								<element name="uncertaintyOfMeas" type="caaml:NonNegMeasureType" minOccurs="0">
									<annotation>
										<documentation>Uncertainty of individual measurements</documentation>
									</annotation>
								</element>
								<element name="probedThickness" type="caaml:MeasureLengthCmType" minOccurs="0">
									<annotation>
										<documentation>Thickness of layer probed in cm</documentation>
									</annotation>
								</element>
							</sequence>
