							</extension>
						</complexContent>
					</complexType>
				</element>
			</choice>
		</sequence>
		<attribute name="uomWeightHammer" type="caaml:UomWeightType" use="optional" fixed="kg"/>
		<attribute name="uomWeightTube" type="caaml:UomWeightType" use="optional" fixed="kg"/>
		<attribute name="uomDropHeight" type="caaml:UomLengthType" use="optional" fixed="cm"/>
	</complexType>
	<simpleType name="HardnessMethodOfMeasType">
		<union memberTypes="caaml:HardnessMethodOfMeasEnumType gml:NilReasonEnumeration"/>
	</simpleType>
	<simpleType name="HardnessMethodOfMeasEnumType">
		<restriction base="string">
			<enumeration value="SnowMicroPen"/>
			<enumeration value="Ram Sonde"/>
			<enumeration value="Push-Pull Gauge"/>
		</restriction>
	</simpleType>
	<!-- EMBEDDED STRENGHT PROFILES -->
	<complexType name="StrengthProfileType" final="#all">
		<sequence>
			<element name="strengthMetaData">
				<complexType>
					<complexContent>
						<extension base="caaml:MetaDataBaseType">
							<sequence>
								<element name="strengthType" type="caaml:StrengthEnumType">
									<annotation>
										<documentation>Type of strength</documentation>
									</annotation>
								</element>
								<element name="methodOfMeas" type="caaml:StrengthMethodOfMeasType">
									<annotation>
										<documentation>Method of measurement</documentation>
									</annotation>
								</element>
								<element name="uncertaintyOfMeas" type="caaml:MeasurePressureType" minOccurs="0">
									<annotation>
										<documentation>Uncertainty of individual measurements</documentation>
									</annotation>
								</element>
								<element name="probedArea" type="caaml:MeasureAreaType" minOccurs="0">
